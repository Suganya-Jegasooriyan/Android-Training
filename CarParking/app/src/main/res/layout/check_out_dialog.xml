<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/check_out_car_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/car_no"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/check_out_mobile_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mobilenumber"
        app:layout_constraintTop_toBottomOf="@+id/check_out_car_number"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/in_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/checkIn"
        app:layout_constraintTop_toBottomOf="@+id/check_out_mobile_number"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/check_out_slot_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/slotNumber"
        app:layout_constraintTop_toBottomOf="@+id/in_time"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/check_out_amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/slotNumber"
        app:layout_constraintTop_toBottomOf="@+id/check_out_slot_number"
        app:layout_constraintStart_toStartOf="parent"
        />
    <Button
        android:id="@+id/check_out_ok_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ok"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>